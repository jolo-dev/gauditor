{
  "name": "codecommit-auditor",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "sst build",
    "console": "sst console",
    "deploy": "sst deploy",
    "dev": "sst dev",
    "remove": "sst remove",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install",
    "format": "yarn biome format . --write",
    "lint": "yarn biome check . --apply",
    "lint:unsafe": "yarn biome check . --apply-unsafe",
    "test": "pnpm test -r"
  },
  "devDependencies": {
    "@biomejs/biome": "1.1.2",
    "@tsconfig/node18": "18.x",
    "@vitest/coverage-v8": "^0.34.1",
    "husky": "8.x",
    "lint-staged": "13.2.x",
    "sst": "~2.31.0",
    "typescript": "5.x",
    "vitest": "^0.34.1"
  }
}